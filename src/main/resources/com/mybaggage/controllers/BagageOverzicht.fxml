<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mybaggage.controllers.BagageOverzichtController">
    <children>
        <BorderPane prefHeight="768.0" prefWidth="1366.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <left>
                <VBox prefHeight="704.0" prefWidth="143.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="btn_bagageToevoegen" mnemonicParsing="false" onAction="#bagageToevoegen" onMouseClicked="#bagageToevoegen" prefHeight="27.0" prefWidth="143.0" text="Bagage Toevoegen" />
                        <Button fx:id="btn_bagageWijzigen" mnemonicParsing="false" onAction="#bagageWijzigen" prefHeight="27.0" prefWidth="143.0" text="Bagage Wijzigen" />
                        <Button fx:id="btn_bagageVerwijderen" mnemonicParsing="false" onAction="#bagageVerwijderen" prefHeight="27.0" prefWidth="143.0" text="Bagage Verwijderen" />
                    </children>
                </VBox>
            </left>
            <center>
                <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <TableView fx:id="tabelBagage" editable="true" prefHeight="633.0" prefWidth="1266.0">
                            <columns>
                                <TableColumn fx:id="kolomnaam" editable="false" minWidth="-1.0" prefWidth="200.0" text="Naam" />
                                <TableColumn fx:id="kolomkleur" editable="false" prefWidth="200.0" text="Kleur" />
                                <TableColumn fx:id="kolomgrootte" editable="false" minWidth="-1.0" prefWidth="200.0" text="Grootte (L/B/H in cm)" />
                                <TableColumn fx:id="kolomsoortbagage" editable="false" prefWidth="200.0" text="Soort Bagage" />
                                <TableColumn fx:id="kolomstatus" editable="false" prefWidth="200.0" text="Status" />
                                <TableColumn fx:id="kolomRegistratieNummer" prefWidth="222.0" text="Registratie Nummer" />
                            </columns>
                        </TableView>
                    </children>
                </StackPane>
            </center>
            <top>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="47.0" prefWidth="1382.0" text="Mijn Bagage" textAlignment="CENTER" textFill="#121111">
                    <font>
                        <Font name="Apple Braille" size="36.0" />
                    </font>
                </Label>
            </top>
            <bottom>
                <Label text="Status Message" BorderPane.alignment="CENTER" />
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
